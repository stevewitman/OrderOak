<% flash.each do |key, value| %>
<p>
  <%= value %>
</p>
<% end %>
<h1>Edit Store</h1>
<%= form_for @store do |f| %>
<p>
  <%= f.label :name %><br>
  <%= f.text_field :name %>
</p>
<p>
  <%= f.label :address %><br>
  <%= f.text_field :address %>
</p>
<p>
  <%= f.label :city %><br>
  <%= f.text_field :city %>
</p>
<p>
  <%= f.label :state %><br>
  <%= f.text_field :state %>
</p>
<p>
  <%= f.label :zip %><br>
  <%= f.text_field :zip %>
</p>
<p>
  <%= f.label :phone %><br>
  <%= f.text_field :phone %>
</p>
<p>
  <%= f.label :website %><br>
  <%= f.text_field :website %>
</p>
<p>
  <%= f.submit "Update" %>
</p>
<p>
  <%= link_to "Back", stores_path %> |
  <%= link_to "Delete", store_path(@store), :confirm => "Are you sure?", :method => :delete %>
</p>

<% end %>
